# Get EKS credentials
if [[ ! -f /root/.kube/config ]]; then
    aws eks --region ${AWS_REGION} update-kubeconfig --name ${EKS_CLUSTER}
fi

if [[ "$(/clisso status 2>&1 | grep ${AWS_PROFILE} | wc -l)" -eq "0" ]]; then
   awslogin
fi

alias s3='aws s3'

function s3cat() {
   aws s3 cp $1 -
}

function s3zcat() {
   aws s3 cp $1 - | zcat
}
